/* 启动页样式 - 智安盾品牌展示 */
.splash-container {
  width: 100%;
  height: 100vh;
  display: flex;
  justify-content: center;
  align-items: flex-start;
  padding-top: 20vh;
  background-color: #ffffff;
  margin: 0;
  padding: 0;
}

.brand-image {
  width: 100%;
  height: 95%;
  object-fit: contain;
  margin-top: -1vh;
}